{
   "manifest_version":2,
   "name":"FC1extension",
   "description":"Puts margins from FlipChat1 into a sortable table.",
   "version":"1.1",
   "content_scripts":[
      {
         "matches":[
            "https://www.flipchat1rs.com/forum/viewtopic.php?f=13*"
         ],
         "js":[
            "jquery-3.1.1.min.js",
            "jquery.tablesorter.js",
            "content.js"
         ]
      }
   ],
   "browser_action":{
      "default_icon":"favicon.png"
   },
   "background":{
      "scripts":[
         "background.js"
      ]
   },
   "icons":{
      "128":"icon.png"
   }
}